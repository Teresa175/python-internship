a=int(input("Enter the number of values to be in the list:"))
L=list(range(0,a))
print(L)
L.append(9) # Add an item in to the list (using function)
print("Appended List:",L)
del L[9] #Delete (using function)
print("The List after deletion:",L)
maximum=max(L) #Store the largest number from the list to a variable
print("The maximum value in the list is :",maximum)
minimum=min(L) #Store the Smallest number from the list to a variable
print("The minimum value in the list is ",minimum)
 

#2 reverse the created tuple
t = ('a', 28, "bcd")
z = reversed(t)
print(tuple(z))

# 3 convert tuple to list
Tup1 = ('a', 28, "bcd")
List1 = list(Tup1)
print(List1)
