#1
a=int(input("Enter the first number: "))
b=int(input("Enter the second number: "))
def X(a,b):
    print("Addition=",a+b)
    print("Subtraction=",a-b)
    print("Division=",a//b)
    print("Multiplication=",a*b)
X(a,b)

